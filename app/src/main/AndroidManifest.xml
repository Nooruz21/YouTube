<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sixthmsecondhmyoutubeapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".youtube.App"
        android:allowBackup="true"
        android:icon="@drawable/ic_social_media_logo_video_player_icon_icons_com_56665"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_social_media_logo_video_player_icon_icons_com_56665"
        android:supportsRtl="true"
        android:theme="@style/Theme.SixthMSecondHMYoutubeApp">
        <activity
            android:name=".youtube.ui.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>